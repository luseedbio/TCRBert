{
  "dash": {
    "loaders": ["dash"],
    "n_cdr3b_cutoff": 20,
    "generate_negatives": true,
    "vocab": "iupac",
    "max_len": 40,
    "result": "../output/dash.data.json"
  },
  "dash_vdjdb_mcpas": {
    "loaders": ["dash", "vdjdb", "mcpas"],
    "n_cdr3b_cutoff": 20,
    "generate_negatives": true,
    "vocab": "iupac",
    "max_len": 40,
    "result": "../output/dash_vdjdb_mcpas.data.json"
  },
  "iedb_sars2": {
    "loaders": ["iedb_sars2"],
    "n_cdr3b_cutoff": 20,
    "generate_negatives": true,
    "query": "(epitope_species == 'SARS-CoV2') and (epitope != 'YLQPRTFLL') and (epitope.str.len() <= 15)",
    "vocab": "iupac",
    "max_len": 40,
    "result": "../output/iedb_sars2.data.json"
  },

  "shomuradova": {
    "loaders": ["shomuradova"],
    "n_cdr3b_cutoff": 20,
    "query": "(epitope != 'RLQSLQTYV')",
    "generate_negatives": true,
    "vocab": "iupac",
    "max_len": 40,
    "result": "../output/shomuradova.data.json"
  },
  "immunecode": {
    "loaders": ["immunecode"],
    "n_cdr3b_cutoff": null,
    "generate_negatives": false,
    "vocab": "iupac",
    "max_len": 40,
    "result": "../output/immunecode.data.json"
  },
  "immunecode002_1": {
    "loaders": ["immunecode002_1"],
    "n_cdr3b_cutoff": 20,
    "generate_negatives": true,
    "vocab": "iupac",
    "max_len": 40,
    "result": "../output/immunecode002_1.data.json"
  },

  "zhang": {
    "loaders": ["zhang"],
    "n_cdr3b_cutoff": 10,
    "generate_negatives": false,
    "vocab": "iupac",
    "max_len": 40,
    "result": "../output/zhang.data.json"
  },
  "nettcr_train": {
    "loaders": ["nettcr_train"],
    "n_cdr3b_cutoff": 10,
    "generate_negatives": false,
    "vocab": "iupac",
    "max_len": 40,
    "result": "../output/nettcr_train.data.json"
  },
  "nettcr_eval": {
    "loaders": ["nettcr_eval"],
    "n_cdr3b_cutoff": 10,
    "generate_negatives": false,
    "vocab": "iupac",
    "max_len": 40,
    "result": "../output/nettcr_eval.data.json"
  }



